{
  "name": "elm-redux",
  "version": "0.0.0",
  "description": "## TOC",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:lint",
    "build": "npm run build:prod",
    "build:dev": "npm run build:webpack:dev",
    "build:prod": "npm run build:webpack:prod",
    "build:webpack:dev": "webpack -d",
    "build:webpack:prod": "webpack -p",
    "serve": "npm run serve:dev",
    "serve:dev": "npm run copy-to-build && webpack-dev-server",
    "copy-to-build": "cp ./index.html ./build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "standard": {
    "ignore": [
      "/build/"
    ],
    "globals": [
      "__DEV__",
      "__TESTING__"
    ],
    "rules": {
      "arrow-parens": 0,
      "babel/arrow-parens": 0
    },
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoeffel/react-redux-bp.git"
  },
  "author": "Christoph Hermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoeffel/react-redux-bp/issues"
  },
  "homepage": "https://github.com/stoeffel/react-redux-bp#readme",
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.1",
    "redux-actions": "^0.9.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "ramda": "^0.21.0",
    "serve": "^1.4.0",
    "standard": "^6.0.8",
    "standard-format": "^2.1.1",
    "watch": "^0.17.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
